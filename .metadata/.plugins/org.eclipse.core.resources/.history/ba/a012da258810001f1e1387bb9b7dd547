package May12;

public class duplicate_characters_in_string {

	public static void main(String[] args) {

		String name="google";
		char ch = 0,ch1;
		Boolean duplicate;
		for(int i=0;i<name.length();i++)
		{
			ch = name.charAt(i);
//			System.out.println(ch);
			duplicate = false;
			for(int j=i+1;j<name.length();j++)
			{
				ch1 = name.charAt(j);
//				System.out.println(ch1);
				if(ch==ch1) 
				{
					duplicate=true;
					
				}
				
				
			}
			if(duplicate=true)
			{
				System.out.println(ch);
			}
			
		}
		
		
		
	}

}
