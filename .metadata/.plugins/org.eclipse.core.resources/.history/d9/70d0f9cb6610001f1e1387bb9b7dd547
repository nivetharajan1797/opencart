package May12;

public class reverse_string {

	public static void main(String[] args)
	{
		String s ="India";
		System.out.println(s.length());
		int n = s.length();
		String rev = "";
		char ch;
		for(int i=n;i<0;i--) {

			ch = s.charAt(i);
			System.out.println(ch);

			rev = rev + ch;
			
		}
		System.out.println(rev);
		
	}
}
