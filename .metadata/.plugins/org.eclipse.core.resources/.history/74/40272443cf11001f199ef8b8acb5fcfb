package Numbers;

public class amstrong {

	public static void main(String[] args) {

		int am = 153;
		int d;
		double rev =0;
		while(am>0)
		{
			d = am%10;
			rev = rev*10 + Math.pow(3, d);
			am = am/10;
		}
		if(rev == am)
		{
			System.out.println("Amstrong number");
		}
		else {
			
			System.out.println("Not a Amstrong number");
		}
	}

}
