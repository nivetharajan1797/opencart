package May12;

public class swap_strings {

	public static void main(String[] args)
	{
		String a = "Nivetha";
		String b = "Rajan";
		a = a + b;
		System.out.println(a);
		b = a.substring(0, a.length()-b.length());
		System.out.println(a);
		a = a.substring(b.length());
		System.out.println(a);
		
	}
}
