package May12;

public class reverse_string {

	public static void main(String[] args)
	{
		String s ="India";
		int n = s.length();
		String rev = " ";
		char[] ch = s.toCharArray();;
		for(int i=n-1;i<0;i--) 
		{

			System.out.println(i);

			rev = rev + ch[i];
			
		}
		System.out.println(rev);
		
	}
}
