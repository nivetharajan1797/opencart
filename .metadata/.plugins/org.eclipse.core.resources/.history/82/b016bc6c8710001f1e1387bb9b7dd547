package May12;

public class duplicate_characters_in_string {

	public static void main(String[] args) {

		String name="goo";
		char ch,ch1;
		Boolean duplicate;
		for(int i=0;i<name.length();i++)
		{
			ch = name.charAt(i);
			
			duplicate = false;
			for(int j=1;j<name.length();j++)
			{
				ch1 = name.charAt(j);
				if(ch==ch1) 
				{
					duplicate=true;
					i=0;
				}
				
				
			}
			if(duplicate=true)
			{
				System.out.println(ch);
			}
			
			
		}
		
		
	}

}
